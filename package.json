    "sql",
    "parser",
    "query",
    "firebase",
    "emulator",
    "firequery"
  ],
  "main": "out/firequery.js",
  "typings": "out/index.d.ts",
  "author": "Eperson Mayrink <epersoncmxf@gmail.com>",
